{
  "name": "color-dropper",
  "description": "A chrome extension for color dropper",
  "version": null,
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "color-dropper"
  },
  "permissions": ["contextMenus", "activeTab", "clipboardWrite"]
}
