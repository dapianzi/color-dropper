{
  "name": "color-dropper",
  "description": "A chrome extension for color dropper",
  "version": 1.0,
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "color-dropper"
  },
  "permissions": ["activeTab"]
}
